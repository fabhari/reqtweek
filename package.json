{
  "name": "reqtweek",
  "displayName": "ReqTweek",
  "version": "0.0.4",
  "description": "ReqTweek enables per-tab proxying with custom headers for web app testing. Supports session saving and restoring in new tabs.",
  "author": "fabhari36@gmail.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-alpha.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.3",
    "bootstrap": "^5.3.3",
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "18.2.0",
    "react-instantsearch": "^7.15.3",
    "react-search-box": "^3.0.0",
    "react-toastify": "^11.0.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "declarativeNetRequest",
      "sidePanel",
      "storage",
      "debugger",
      "system.display"
    ]
  }
}
